{
  "name": "drupal/hook_event_dispatcher",
  "description": "Dispatches events for several drupal core hooks.",
  "type": "drupal-module",
  "license": "GPL-2.0+",
  "require-dev": {
    "drupal/core": "^8.2",
    "phpunit/phpunit": "^5.7",
    "drupal/coder": "^8.2",
    "phpmd/phpmd": "^2.6",
    "jakub-onderka/php-parallel-lint": "^0.9.2",
    "jakub-onderka/php-console-highlighter": "^0.3.2"
  },
  "minimum-stability": "alpha",
  "scripts": {
    "test": [
      "@phplint",
      "@phpunit",
      "@phpcs",
      "@phpmd"
    ],
    "phplint": "vendor/bin/parallel-lint --exclude vendor/ -e php,module,inc,install,profile,theme .",
    "phpunit": "vendor/bin/phpunit --configuration=phpunit.xml",
    "phpcs": "vendor/bin/phpcs --standard=vendor/drupal/coder/coder_sniffer/Drupal/ruleset.xml --extensions=php,module,inc,install,profile,theme --report=full --warning-severity=0 --ignore=vendor/ .",
    "phpmd": "vendor/bin/phpmd --exclude vendor --suffixes php,module,inc,install,profile,theme . text phpmd.xml",
    "phpcbf": "vendor/bin/phpcbf --standard=vendor/drupal/coder/coder_sniffer/Drupal/ruleset.xml --ignore=vendor/ --extensions=php,module,inc,install,profile,theme ."
  },
  "autoload": {
    "psr-4": {
      "Drupal\\hook_event_dispatcher\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Drupal\\Tests\\hook_event_dispatcher\\": "tests/src/"
    }
  },
  "repositories": {
    "drupal": {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    }
  },
  "suggest": {
    "drupal/token": "Provides additional tokens not supported by core (most notably fields), as well as a UI for browsing tokens."
  }
}
